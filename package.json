{
  "name": "products-page",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 next dev",
    "build": "next build",
    "start": "next start",
    "cp": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "standalone": "node .next/standalone/server.js",
    "clean": "rm -rf node_modules package-lock.json .fleek .vercel",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "dc:build": "DOCKER_BUILDKIT=0 docker compose build",
    "docker:build:push:arm": "docker buildx build -f ./Dockerfile -t nemanjamitic/products-page --platform linux/arm64 --push .",
    "docker:build:push:x86": "docker buildx build -f ./Dockerfile -t nemanjamitic/products-page --platform linux/amd64 --push ."
  },
  "dependencies": {
    "@fleek-platform/next": "^2.2.1",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "0.445.0",
    "next": "^14.2.13",
    "next-themes": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.6",
    "use-debounce": "^10.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/node": "^22.5.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/parser": "^8.6.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.24",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "typescript": "5.5.3"
  }
}
